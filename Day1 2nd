import java.util.*;
public class comp{
	public static void main(String[] args){
		Scanner sc=new Scanner(System.in);
		String a,b;
		System.out.print("Enter a string 1 : ");
		a=sc.nextLine();
		System.out.print("Enter a string 2 : ");
		b=sc.nextLine();
		if(a.equals(b))
		{
			System.out.print("Valid UserName");
		}
		else
		{
			System.out.print("Invalid username !!!");
		}

	}
}

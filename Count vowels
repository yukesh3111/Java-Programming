import java.util.*;
public class vowels{
	public static void main(String arg[]){
		Scanner sc=new Scanner(System.in);
		System.out.print("Enter String : ");
		String a=sc.nextLine();
		int count=0;
		for(int i=0;i<a.length();i++)
		{
			char s=a.charAt(i);
			if(s=='a'|| s=='e'|| s=='i'|| s=='o'|| s=='u')
			{
				count++;
			}
		}
		System.out.print("Vowels are "+count);
	}
}
